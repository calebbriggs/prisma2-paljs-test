{
  "name": "script",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@graphql-codegen/typescript-react-query": "^1.3.1",
    "prisma": "2.23.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "start:watch": "nodemon",
    "gen:ui": "graphql-codegen --config ./src/uiSdk/codegen.yaml"
  },
  "dependencies": {
    "@graphql-tools/merge": "^6.2.14",
    "@paljs/admin": "^3.3.4",
    "@paljs/plugins": "^3.3.3",
    "@prisma/client": "2.23.0",
    "apollo-server": "^2.25.0",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.10",
    "react-query": "^3.16.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./src/**/*js",
      "./src/**/*ts",
      "./src/**/*graphql"
    ],
    "exec": "npm start",
    "ext": "ts,js,graphql"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
